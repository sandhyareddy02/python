#In range():
for value in range(1,20):
	print(value)

#In range() to list():
values=list(range(1,20))
print(values)

#1 to 1 million:
for value in range(1,1000000):
	print(value)

#odd_no 1 to 20:
odd_no=list(range(1,20,2))
print(odd_no)

#multiplies of 3 from 3 to 30:
mul_3=list(range(3,30,3))
print(mul_3)

#cube from 1 to 10:
cubes=[]
for value in range(1,10):
	cubes.append(value**3)
print(cubes)

#list comprehensions in cube:
cubes=[value**3 for value in range(1,11)]
print(cubes)
